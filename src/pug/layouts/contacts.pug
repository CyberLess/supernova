+b.SECTION.contacts
	+e.wrap.wrap
		+e.box
			+e.map#map(data-lat="55.751574" data-lon="37.573856")
			+e.inside
				+e.circle._contacts
					+e.inner
						+e.title.h6 Контакты
						+e.list
							+e.list-item.middle
								| Санкт-Петербург
								br
								| Итальянская 13
							+e.list-item.middle
								| ПН-ПТ 10:00-20:00
								br
								| СБ-ВС выходные
							+e.list-item.middle
								| +7 812 800 80 80
				+e.circle._recall._right._top
					+e.inner
						+e.manager
							+e.IMG.manager-image.object-fit(src="/img/manager.png")
						+e.title.h6 остались вопросы?
						+e.text.p
							| Закажите обратный 
							br
							| звонок менеджеру Юлии
						+e.arrow
							+icon('arrow')

